terno = '',\r\n                    direccionnueva.value.email = '',\r\n                    direccionnueva.value.domicilio = ''\r\n            } catch (error) {\r\n                console.log(\"no se puede crear cliente nuevo\", error);\r\n            }\r\n        };\r\n        return {\r\n            direccionnueva,\r\n            Crear\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<template>\r\n    <main>\r\n        <h1>Nueva Direcion</h1>\r\n        <form @submit.prevent=\"Crear\" action=\"\">\r\n            <div>\r\n                <label for=\"nombre\">Nombre:</label>\r\n                <input name=\"nombre\" v-model=\"direccionnueva.title\" type=\"text\" required>\r\n                <label for=\"apellidomaterno\">Apellido Materno:</label>\r\n                <input name=\"apellidomaterno\" v-model=\"direccionnueva.apellidomaterno\" type=\"text\">\r\n                <label for=\"apellidopaterno\">Apellido Paterno:</label>\r\n                <input name=\"apellidopaterno\" v-model=\"direccionnueva.apellidopaterno\" type=\"text\">\r\n                <label for=\"domicilio\">Dirección:</label>\r\n                <input name=\"domicilio\" v-model=\"direccionnueva.domicilio\" type=\"text\">\r\n                <label for=\"email\">E-mail:</label>\r\n                <input name=\"email\" v-model=\"direccionnueva.email\" type=\"text\">\r\n                <div>\r\n                    <button type=\"submit\">Guardar</button>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    </main>\r\n</template>\r\n\r\n<style></style>"],"mappings":"AACA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,cAAa,GAAIH,GAAG,CAAC;MACvBI,MAAM,EAAE,EAAE;MAAEC,eAAe,EAAE,EAAE;MAAEC,eAAe,EAAE,EAAE;MAAEC,KAAK,EAAE,EAAE;MAAEC,SAAS,EAAE;IAChF,CAAC,CAAC;IACF,MAAMC,KAAI,GAAI,MAAAA,CAAA,KAAY;MACtB,IAAI;QACA,MAAMR,KAAK,CAACS,IAAI,CAAC,mCAAmC,EAAEP,cAAc,CAACQ,KAAK,CAAC;QAC3EC,UAAU,CAAC,MAAM;UACbC,KAAK,CAAC,kBAAkB,CAAC;QAC7B,CAAC,EAAE,GAAG,CAA